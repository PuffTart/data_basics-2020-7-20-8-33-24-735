    "def fibonList(num):   #定义斐波那契数列\n",
    "    fibonList = []\n",
    "    for i in range(0, num):\n",
    "        if len(fibonList) < 2:\n",
    "            fibonList.append(1)\n",
    "            continue\n",
    "        fibonList.append(fibonList[-1]+fibonList[-2])   # 巧用列表\n",
    "    return fibonList\n",
    "fibonList(5)"
   ]
